<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <beans>
        <bean id="player" class="org.academiadecodigo.bootcamp.model.Player">
            <constructor-arg name="name" value="BIG"/>
            <constructor-arg name="computer" ref="computer"/>
        </bean>

        <bean id="computer" class="org.academiadecodigo.bootcamp.model.FatelaComputer">

        </bean>
    </beans>
</beans>